{% include 'OpenOrchestraBackofficeBundle:BackOffice:Underscore/Jarvis/header._tpl.twig' %}

<% if (typeof multiLanguage != 'undefined') { %>
    <ul id="entity-languages" class="nav nav-tabs">
    </ul>
<% } %>

<% if (typeof duplicate != 'undefined') { %>
    <span id="entity-duplicate" data-text="{{ ('open_orchestra_backoffice.element.version.create') | trans({}, 'messages', language) }}"></span>
<% } %>

<% if (typeof multiVersion != 'undefined') { %>
<div id="additionalContent" class="smart-form">
    <label class="select">
        <select id="version-selectbox" class="version-selectbox-<%= cid %>"/>
        <i></i>
    </label>
</div>
<% } %>

<div role="container">
    <%= html %>
    {% include 'OpenOrchestraBackofficeBundle:BackOffice:Underscore/backToList.html.twig' %}
</div>

{% include 'OpenOrchestraBackofficeBundle:BackOffice:Underscore/Jarvis/footer._tpl.twig' %}
