<span rel="tooltip" data-placement="top" data-original-title="
{{ 'php_orchestra_backoffice.node.last_update.at'| trans({}, 'messages', language) }}
<%= node.get('updated_at') %>
{{ 'php_orchestra_backoffice.node.last_update.by'| trans({}, 'messages', language) }}
<% if (node.get('updated_by') !== undefined ) { %>
    <%= node.get('updated_by') %>
<% } else { %>
    anonymous
<% } %>
">
    <%= node.get('name') %>
    <% if (node.get('node_type') == 'page') { %>
        ({{ 'php_orchestra_backoffice.node.version.display' | trans({}, 'messages', language) }} : #<%= node.get('version') %>)
    <% } %>
</span>
