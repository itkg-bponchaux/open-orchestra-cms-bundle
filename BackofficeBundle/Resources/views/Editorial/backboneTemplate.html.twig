<script type="text/html" id="nodeView">
    <a href="#" class="ajax-node-duplicate-<%= node.cid %>">{{ ('php_orchestra_backoffice.node.version.create') | trans }}</a>
    <% include headInclude %>
    <div id="generated-title" style="display: none">
        <%= node.get('name') %>
        ({{ 'php_orchestra_backoffice.node.version.display' | trans }} : #<%= node.get('version') %>)
        {{ 'php_orchestra_backoffice.node.last_update.at'| trans }}
        <%= node.get('updated_at') %>
        {{ 'php_orchestra_backoffice.node.last_update.by'| trans }}
        <%= node.get('updated_by') %>
    </div>
    <div role="container">
        <div class="ui-model">
            <span class="title"></span>
            <span class="preview"></span>
                <span class="action">
                    <i class="fa fa-cog <%= node.cid %>"></i>
                </span>
            <ul class="ui-model-areas">
            </ul>
        </div>
    </div>
    <% include footerInclude %>
</script>
<script type="text/html" id="templateView">
    <% include headInclude %>
    <div role="container">
        <div class="ui-model">
            <span class="title"></span>
            <span class="preview"></span>
                <span class="action">
                    <i class="fa fa-cog <%= template.cid %>"></i>
                </span>
            <ul class="ui-model-areas">
            </ul>
        </div>
    </div>
    <% include footerInclude %>
</script>
<script type="text/html" id="areaView">
    <div>
        <span class="title">
            <%= area.get('ui_model').label %> 
            <% if (area.get('ui_model').id) { %>
                id="<%= area.get('ui_model').id %>"
            <% } %>
            <% if (area.get('ui_model').class) { %>
                class="<%= area.get('ui_model').class %>"
            <% } %>
        </span>
        <span class="preview"></span>
            <span class="action">
                <i class="fa fa-wrench area-param-<%= area.cid %>"></i>
                <i class="fa fa-trash-o area-remove-<%= area.cid %>"></i>
            </span>
        <ul class="ui-model-areas">
        </ul>
        <ul class="ui-model-blocks resizable-<%= cid %>">
        </ul>
    </div>
</script>
<script type="text/html" id="blockView">
    <div class="<%= block.get('method') %>" data-node-id="<%= block.get('node_id') %>" data-block-id="<%= block.get('block_id') %>">
        <span class="title"><%= block.get('ui_model').label %></span>
        <div class="preview">
            <%= block.get('ui_model').html %>
        </div>
        <span class="action">
            <i class="block-param fa fa-wrench"></i>
            <i class="block-remove fa fa-trash-o"></i>
        </span>
    </div>
</script>

<script type="text/html" id="tableviewCollectionView">
    <% include headInclude %>
    <div role="container">
        <table id="tableviewCollectionTable">
            <thead>
                <tr>
                    <% _.each(displayedElements, function(displayedElement) { %>
                        <td><%= displayedElement %></td>
                    <% }); %>
                    <td>Actions</td>
                </tr>
            </thead>
            <tbody>
            </tbody>
        </table>
        <% if (typeof links._self_add !== 'undefined' ) { %>
            <div>
                <a href="#" class="ajax-add-<%= cid %>">{{ 'php_orchestra_backoffice.form.add'|trans }}</a>
            </div>
        <% } %>
    </div>
    <% include footerInclude %>
</script>

<script type="text/html" id="tableviewView">
    <td><%= value %></td>
</script>

<script type="text/html" id="tableviewActions">
    <td>
        <% if (typeof links._self_form !== 'undefined' ) { %>
            <a href="#" class="ajax-edit">{{ 'php_orchestra_backoffice.form.edit'|trans }}</a>
        <% } %>
        <% if (typeof links._self_delete !== 'undefined' ) { %>
            <a href="#" class="ajax-delete">{{ 'php_orchestra_backoffice.form.delete'|trans }}</a>
        <% } %>
    </td>
</script>

<script type="text/html" id="fullPageFormView">
    <% include headInclude %>
    <div role="container">
        <%= html %>
        <a
            href="#<%= listUrl %>"
            class="btn btn-primary pull-right">
            Back to list
        </a>
    </div>
    <% include footerInclude %>
</script>
